# Kali Robot Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git!

# MCP Server
MCP_API_KEY=your_mcp_api_key_here

# n8n Webhooks (optional, for authenticated webhooks)
N8N_WEBHOOK_KEY=your_n8n_webhook_key_here

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DEFAULT_REGION=us-east-1

# Raspberry Pi Specific
KALI_DEBUG=false
KALI_LOG_LEVEL=INFO

# Optional: SSH connection details for remote management
PI_SSH_HOST=raspberrypi.local
PI_SSH_USER=kali
PI_SSH_PORT=22
